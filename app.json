{
  "expo": {
    "name": "Green.ME",
    "slug": "green-me",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "0.9.1",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/images/ios.icon.png",
      "bundleIdentifier": "green.me",
      "supportsTablet": true,
      "buildNumber": "41"
    },
    "android": {
      "icon": "./assets/images/android.icon.png",
      "useNextNotificationsApi": true,
      "package": "green.me",
      "versionCode": 41
    },
    "userInterfaceStyle": "automatic",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "green",
            "project": "green.me",
            "authToken": null
          }
        }
      ]
    }
  }
}
